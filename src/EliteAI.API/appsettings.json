{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Supabase": {
    "Url": "https://your-project.supabase.co",
    "ServiceRoleKey": "your-service-role-key",
    "AnonKey": "your-anon-key",
    "JwtSecret": "your-jwt-secret"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=db.your-project.supabase.co;Database=postgres;Username=postgres;Password=your-password;SSL Mode=Require;Trust Server Certificate=true"
  },
  "Jwt": {
    "Key": "your-secret-key-here",
    "Issuer": "your-issuer-here",
    "Audience": "your-audience-here",
    "ExpiryInMinutes": 60
  },
  "RabbitMQ": {
    "Url": "amqp://guest:guest@localhost:5672",
    "Password": "guest"
  },
  "Anthropic": {
    "ApiKey": "your-anthropic-api-key"
  },
  "Sentry": {
    "Dsn": "your-sentry-dsn"
  }
}
