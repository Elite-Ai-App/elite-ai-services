{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Supabase": {
    "Url": "https://your-project.supabase.co",
    "AnonKey": "your-anon-key",
    "ServiceRoleKey": "your-service-role-key"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=db.xxxxx.supabase.co;Database=postgres;Username=postgres;Password=xxxxx",
    "DirectConnection": "Host=db.xxxxx.supabase.co;Database=postgres;Username=postgres;Password=xxxxx"
  },
  "Anthropic": {
    "ApiKey": "your-anthropic-api-key"
  },
